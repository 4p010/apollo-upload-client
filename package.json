{
  "name": "apollo-upload-client",
  "version": "8.1.0",
  "description": "Enhances Apollo Client for intuitive file uploads via GraphQL mutations.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/apollo-upload-client",
  "homepage": "https://github.com/jaydenseric/apollo-upload-client#readme",
  "bugs": "https://github.com/jaydenseric/apollo-upload-client/issues",
  "keywords": [
    "apollo",
    "client",
    "link",
    "graphql",
    "file",
    "upload",
    "multipart",
    "react",
    "native"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/main",
  "module": "lib/module",
  "sideEffects": false,
  "engines": {
    "node": ">=6.10"
  },
  "browserslist": ">1%",
  "dependencies": {
    "apollo-link": "^1.2.2",
    "apollo-link-http-common": "^0.2.4",
    "extract-files": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "apollo-link": "^1.2.2",
    "babel-eslint": "^8.2.5",
    "eslint": "^5.3.0",
    "eslint-config-env": "^1.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.1",
    "graphql": "^0.13.2",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "size-limit": "^0.19.2"
  },
  "scripts": {
    "prepare": "FORCE_COLOR=1 npm-run-all prepare:clean -p prepare:main prepare:module -s prepare:prettier --aggregate-output",
    "prepare:clean": "rm -r lib; mkdir lib lib/main lib/module",
    "prepare:main": "babel src -d lib/main",
    "prepare:module": "MODULE=1 babel src -d lib/module",
    "prepare:prettier": "prettier 'lib/**/*.js' --write",
    "test": "FORCE_COLOR=1 run-p test:* -c --aggregate-output",
    "test:eslint": "eslint . --ext mjs,js",
    "prepublishOnly": "run-s prepare test",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:size": "size-limit",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{mjs,js}": "eslint",
    "*.{json,yml,md}": "prettier -l"
  },
  "eslintIgnore": [
    "lib"
  ],
  "eslintConfig": {
    "extends": [
      "env"
    ],
    "rules": {
      "import/default": "off",
      "import/no-named-as-default-member": "off"
    }
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  },
  "size-limit": [
    {
      "name": "ESM",
      "path": "lib/index.mjs",
      "limit": "1 KB",
      "ignore": [
        "apollo-link",
        "apollo-link-http-common"
      ]
    },
    {
      "name": "CJS",
      "path": "lib/index.js",
      "limit": "1 KB",
      "ignore": [
        "apollo-link",
        "apollo-link-http-common"
      ]
    }
  ]
}
